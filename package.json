{
  "name": "auto-agi-builder",
  "version": "1.0.0",
  "description": "Auto AGI Builder - An AI-powered prototype development platform",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd frontend && npm install",
    "dev:backend": "cd app && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd app && python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "start:frontend": "cd frontend && npm run start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd app && python -m flake8",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd app && python -m pytest",
    "test": "npm run test:frontend && npm run test:backend",
    "deploy": "vercel --prod",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CleanExpo/auto-agi-builder.git"
  },
  "keywords": [
    "ai",
    "prototype",
    "builder",
    "fastapi",
    "nextjs",
    "react"
  ],
  "author": "Auto AGI Builder Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CleanExpo/auto-agi-builder/issues"
  },
  "homepage": "https://github.com/CleanExpo/auto-agi-builder#readme",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "vercel": "^30.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "frontend/**/*.{js,jsx,ts,tsx}": [
      "cd frontend && npm run lint"
    ],
    "app/**/*.py": [
      "python -m flake8"
    ]
  }
}
