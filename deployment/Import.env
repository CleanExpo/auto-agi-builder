# Environment Variables for Auto AGI Builder
# Copy and paste this entire content into Vercel's "Import" functionality
# Format: NAME=VALUE (one per line)

# Critical variables for fixing context provider errors
DISABLE_STATIC_GENERATION=true

# Project identification
VERCEL_PROJECT_ID=prj_7uKXTp60gosR1DMXBpOaI0hTyPEO

# Environment configuration
NEXT_PUBLIC_ENVIRONMENT=production

# API configuration
NEXT_PUBLIC_API_URL=https://api.autoagibuilder.app

# Domain settings
NEXT_PUBLIC_APP_URL=https://auto-agi-builder.app

# Authentication settings (you may need to update these based on your specific auth setup)
# NEXT_PUBLIC_AUTH_ENABLED=true
# AUTH_SECRET=your-auth-secret-here
# NEXTAUTH_URL=https://auto-agi-builder.app

# Custom domains
CUSTOM_DOMAIN=auto-agi-builder.app
WWW_DOMAIN=www.auto-agi-builder.app

# Feature flags
NEXT_PUBLIC_FEATURE_COLLABORATIONS=true
NEXT_PUBLIC_FEATURE_EXPORT_SCHEDULING=true
NEXT_PUBLIC_FEATURE_NOTIFICATIONS=true

# Performance settings
NEXT_PUBLIC_IMAGE_OPTIMIZATION=true
