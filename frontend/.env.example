# Auto AGI Builder - Environment Variables
# Copy this file to .env.local for local development
# Configure these variables in your Vercel project settings for deployment

# API Configuration
NEXT_PUBLIC_API_URL=https://api.yourdomain.com
# For local development, use:
# NEXT_PUBLIC_API_URL=http://localhost:8000

# Application URL
NEXT_PUBLIC_APP_URL=https://auto-agi-builder.vercel.app
# For local development, use:
# NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_BETA_FEATURES=false

# Authentication
NEXTAUTH_URL=https://auto-agi-builder.vercel.app
NEXTAUTH_SECRET=your-nextauth-secret  # Generate with: openssl rand -base64 32

# Third-party Service Keys
# Remove comment and add your keys for services you're using
# NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
# NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
# NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-firebase-project-id

# Sentry Error Tracking
# NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Google Analytics
# NEXT_PUBLIC_GA_MEASUREMENT_ID=your-ga-measurement-id
