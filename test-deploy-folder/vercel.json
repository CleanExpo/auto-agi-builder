{
  "version": 2,
  "root": ".",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "skipAutoInstall": false
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_DISABLE_STATIC_GENERATION": "true"
  },
  "routes": [
    {
      "src": "/clients/:id",
      "dest": "/clients/[id]"
    },
    {
      "src": "/projects/:id",
      "dest": "/projects/[id]"
    },
    {
      "src": "/scheduled-exports/:id",
      "dest": "/scheduled-exports/[id]"
    }
  ]
}