{
  "version": 2,
  "public": true,
  "buildCommand": null,
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api-auto-agi-builder.vercel.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": false,
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)\\.(js|css|json)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.js$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)\\.compressed\\.js$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "X-Compressed-By",
          "value": "auto-agi-context-compression"
        }
      ]
    },
    {
      "source": "/compressed-deploy/(.*)",
      "headers": [
        {
          "key": "X-Compressed-By",
          "value": "auto-agi-context-compression"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "alias": [
    "autoagibuilder.app",
    "www.autoagibuilder.app"
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.autoagibuilder.app",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "autoagibuilder.app"
        }
      ]
    },
    {
      "source": "http://:host(.+)",
      "destination": "https://:host",
      "statusCode": 308
    }
  ]
}